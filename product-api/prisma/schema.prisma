generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id          String   @id @default(uuid())
  name        String
  description String
  userId      String
  price       Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
